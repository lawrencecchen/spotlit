#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/lawrencechen/fun/spotlit/node_modules/.pnpm/esbuild@0.14.21/node_modules/esbuild/bin/node_modules:/Users/lawrencechen/fun/spotlit/node_modules/.pnpm/esbuild@0.14.21/node_modules/esbuild/node_modules:/Users/lawrencechen/fun/spotlit/node_modules/.pnpm/esbuild@0.14.21/node_modules:/Users/lawrencechen/fun/spotlit/node_modules/.pnpm/node_modules:/Users/lawrencechen/fun/spotlit/node_modules:/Users/lawrencechen/fun/node_modules:/Users/lawrencechen/node_modules:/Users/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/lawrencechen/fun/spotlit/node_modules/.pnpm/esbuild@0.14.21/node_modules/esbuild/bin/node_modules:/Users/lawrencechen/fun/spotlit/node_modules/.pnpm/esbuild@0.14.21/node_modules/esbuild/node_modules:/Users/lawrencechen/fun/spotlit/node_modules/.pnpm/esbuild@0.14.21/node_modules:/Users/lawrencechen/fun/spotlit/node_modules/.pnpm/node_modules:/Users/lawrencechen/fun/spotlit/node_modules:/Users/lawrencechen/fun/node_modules:/Users/lawrencechen/node_modules:/Users/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/esbuild" "$@"
else
  exec node  "$basedir/../../bin/esbuild" "$@"
fi
