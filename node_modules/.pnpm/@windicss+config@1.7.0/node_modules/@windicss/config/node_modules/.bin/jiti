#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/runner/spotlit/node_modules/.pnpm/jiti@1.12.15/node_modules/jiti/bin/node_modules:/home/runner/spotlit/node_modules/.pnpm/jiti@1.12.15/node_modules/jiti/node_modules:/home/runner/spotlit/node_modules/.pnpm/jiti@1.12.15/node_modules:/home/runner/spotlit/node_modules/.pnpm/node_modules:/home/runner/spotlit/node_modules:/home/runner/node_modules:/home/node_modules:/node_modules:/home/runner/spotlit/node_modules/.pnpm/@windicss+config@1.7.0/node_modules/@windicss/config/node_modules:/home/runner/spotlit/node_modules/.pnpm/@windicss+config@1.7.0/node_modules/@windicss/node_modules:/home/runner/spotlit/node_modules/.pnpm/@windicss+config@1.7.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/runner/spotlit/node_modules/.pnpm/jiti@1.12.15/node_modules/jiti/bin/node_modules:/home/runner/spotlit/node_modules/.pnpm/jiti@1.12.15/node_modules/jiti/node_modules:/home/runner/spotlit/node_modules/.pnpm/jiti@1.12.15/node_modules:/home/runner/spotlit/node_modules/.pnpm/node_modules:/home/runner/spotlit/node_modules:/home/runner/node_modules:/home/node_modules:/node_modules:/home/runner/spotlit/node_modules/.pnpm/@windicss+config@1.7.0/node_modules/@windicss/config/node_modules:/home/runner/spotlit/node_modules/.pnpm/@windicss+config@1.7.0/node_modules/@windicss/node_modules:/home/runner/spotlit/node_modules/.pnpm/@windicss+config@1.7.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../jiti@1.12.15/node_modules/jiti/bin/jiti.js" "$@"
else
  exec node  "$basedir/../../../../../../jiti@1.12.15/node_modules/jiti/bin/jiti.js" "$@"
fi
