#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/lawrencechen/fun/spotlit/node_modules/.pnpm/vite@2.8.0/node_modules/vite/bin/node_modules:/Users/lawrencechen/fun/spotlit/node_modules/.pnpm/vite@2.8.0/node_modules/vite/node_modules:/Users/lawrencechen/fun/spotlit/node_modules/.pnpm/vite@2.8.0/node_modules:/Users/lawrencechen/fun/spotlit/node_modules/.pnpm/node_modules:/Users/lawrencechen/fun/spotlit/node_modules:/Users/lawrencechen/fun/node_modules:/Users/lawrencechen/node_modules:/Users/node_modules:/node_modules:/Users/lawrencechen/fun/spotlit/node_modules/vite/bin/node_modules:/Users/lawrencechen/fun/spotlit/node_modules/vite/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/lawrencechen/fun/spotlit/node_modules/.pnpm/vite@2.8.0/node_modules/vite/bin/node_modules:/Users/lawrencechen/fun/spotlit/node_modules/.pnpm/vite@2.8.0/node_modules/vite/node_modules:/Users/lawrencechen/fun/spotlit/node_modules/.pnpm/vite@2.8.0/node_modules:/Users/lawrencechen/fun/spotlit/node_modules/.pnpm/node_modules:/Users/lawrencechen/fun/spotlit/node_modules:/Users/lawrencechen/fun/node_modules:/Users/lawrencechen/node_modules:/Users/node_modules:/node_modules:/Users/lawrencechen/fun/spotlit/node_modules/vite/bin/node_modules:/Users/lawrencechen/fun/spotlit/node_modules/vite/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../vite/bin/vite.js" "$@"
fi
