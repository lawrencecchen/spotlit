#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/lawrencechen/fun/spotlit/node_modules/.pnpm/windicss@3.4.3/node_modules/windicss/cli/node_modules:/Users/lawrencechen/fun/spotlit/node_modules/.pnpm/windicss@3.4.3/node_modules/windicss/node_modules:/Users/lawrencechen/fun/spotlit/node_modules/.pnpm/windicss@3.4.3/node_modules:/Users/lawrencechen/fun/spotlit/node_modules/.pnpm/node_modules:/Users/lawrencechen/fun/spotlit/node_modules:/Users/lawrencechen/fun/node_modules:/Users/lawrencechen/node_modules:/Users/node_modules:/node_modules:/Users/lawrencechen/fun/spotlit/node_modules/windicss/cli/node_modules:/Users/lawrencechen/fun/spotlit/node_modules/windicss/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/lawrencechen/fun/spotlit/node_modules/.pnpm/windicss@3.4.3/node_modules/windicss/cli/node_modules:/Users/lawrencechen/fun/spotlit/node_modules/.pnpm/windicss@3.4.3/node_modules/windicss/node_modules:/Users/lawrencechen/fun/spotlit/node_modules/.pnpm/windicss@3.4.3/node_modules:/Users/lawrencechen/fun/spotlit/node_modules/.pnpm/node_modules:/Users/lawrencechen/fun/spotlit/node_modules:/Users/lawrencechen/fun/node_modules:/Users/lawrencechen/node_modules:/Users/node_modules:/node_modules:/Users/lawrencechen/fun/spotlit/node_modules/windicss/cli/node_modules:/Users/lawrencechen/fun/spotlit/node_modules/windicss/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../windicss/cli/index.js" "$@"
else
  exec node  "$basedir/../windicss/cli/index.js" "$@"
fi
